q()
#Setting working directory
setwd("/Users/AU775281/Documents/PhD/Acidification/Results/N2O/Raw data")
#libraries
library(HMR)
library(dplyr)
library(ggplot2)
#Runing HRM
res = HMR(
filename = 'Flux R csv.csv',
sep = ',',
LR.always = TRUE,
FollowHMR = TRUE,
pfvar = 0.0001,
IfNoSignal = 'LR',
SatPct = 90,
SatTimeMin = 2
)
#Filter necessary data and renaming
flux.dat <- res[, c("Series", "f0", "Method")]
names(flux.dat)[names(flux.dat) == "f0"] <- "flux"
flux <- data.frame(group = groups) %>%
mutate(flux = flux_subset) %>%
group_by(group) %>%
summarise(
mean_flux = mean(flux, na.rm=TRUE),
sd_flux = sd(flux, na.rm=TRUE)
)
